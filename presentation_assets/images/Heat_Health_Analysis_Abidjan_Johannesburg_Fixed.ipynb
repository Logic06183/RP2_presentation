    "        .filterDate('2021-01-01', '2023-12-31') \\\n",
    "        .select('CO_column_number_density')\n",
    "    \n",
    "    no2_size = no2_collection.size().getInfo()\n",
    "    co_size = co_collection.size().getInfo()\n",
