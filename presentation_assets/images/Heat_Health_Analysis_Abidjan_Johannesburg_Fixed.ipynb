   "source": [
    "# Load vegetation data using current Landsat collection\n",
    "def get_vegetation_data():\n",
    "    \"\"\"Load Landsat NDVI data for vegetation analysis (current collection)\"\"\"\n",
    "    \n",
    "    # Landsat 8-9 Collection 2 Surface Reflectance (current)\n",
    "    landsat = ee.ImageCollection(\"LANDSAT/LC08/C02/T1_L2\") \\\n",
    "        .merge(ee.ImageCollection(\"LANDSAT/LC09/C02/T1_L2\")) \\\n",
    "        .filterDate('2020-01-01', '2023-12-31') \\\n",
    "        .filter(ee.Filter.lt('CLOUD_COVER', 20))\n",
    "    \n",
    "    # Function to calculate NDVI\n",
    "    def calculate_ndvi(image):\n",
    "        # Scale factors for Landsat Collection 2\n",
    "        optical_bands = image.select('SR_B.').multiply(0.0000275).add(-0.2)\n",
    "        \n",
    "        # Calculate NDVI\n",
    "        ndvi = optical_bands.normalizedDifference(['SR_B5', 'SR_B4']).rename('NDVI')\n",
    "        \n",
    "        return image.addBands(ndvi).copyProperties(image, ['system:time_start'])\n",
    "    \n",
    "    # Apply NDVI calculation\n",
    "    landsat_ndvi = landsat.map(calculate_ndvi)\n",
    "    \n",
    "    collection_size = landsat_ndvi.size().getInfo()\n",
    "    print(f\"üì° Loaded Landsat collection: {collection_size} images\")\n",
    "    \n",
    "    return landsat_ndvi\n",
    "\n",
    "# Get vegetation data\n",
    "try:\n",
    "    vegetation_data = get_vegetation_data()\n",
    "    print(\"‚úÖ Vegetation data loaded\")\n",
    "except Exception as e:\n",
    "    print(f\"‚ùå Error loading vegetation data: {e}\")\n",
    "    vegetation_data = None"
